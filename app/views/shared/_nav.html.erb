  <!-- Right Navigation -->
  <div class="navbar-pet-right">
    <% if user_signed_in? %>
      <div class="avatar-container" id="menu-trigger">
        <%= cl_image_tag current_user.photo, class: "nav-avatar" %>
        <i class="fa fa-chevron-down" aria-hidden="true"></i>
        <div class="avatar-menu" id="nav-menu">
          <%= link_to('Dashboard', dashboard_path, class: "menu-link") %>
          <%= link_to('Inbox', root_path, class: "menu-link") %>
          <%= link_to('Logout', destroy_user_session_path, method: :delete, class: "menu-link") %>
        </div>
      </div>
      <%= link_to(new_service_path, class: "btn btn-primary plus") do %>
        <i class="fa fa-plus" aria-hidden="true"></i>
      <% end %>
      <div class="btn btn-primary" id="open-create">MODEL</div>
    <% else %>
      <%= link_to('Sign up', new_user_registration_path, class: "btn btn-primary") %>
      <% if current_page?(root_path) %>
        <%= link_to('Login', new_user_session_path, class: "btn btn-white ", id: "login-btn") %>
      <% else %>
        <%= link_to('Login', new_user_session_path, class: "btn btn-white btn-to-blue") %>
      <% end %>
    <% end %>

  </div>
